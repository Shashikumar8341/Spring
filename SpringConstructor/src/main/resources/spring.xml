<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->
	<bean id="employe" class="com.src.dao.Employe">
		<constructor-arg value="01"></constructor-arg>
		<constructor-arg value="shiva"></constructor-arg>
	</bean>
	<bean id="address" class="com.src.dao.Address">
		<constructor-arg value="01"></constructor-arg>
		<constructor-arg value="shivakumar"></constructor-arg>
		<constructor-arg ref="employe"></constructor-arg>

	</bean>

</beans>